endfor %}\r\n            </select><br>\r\n\r\n            <label for=\"temps\">Temps :</label><br>\r\n            <input type=\"time\" id=\"temps\" name=\"temps\" style=\"width: 300px; height: 40px;\" value=\"{{ tournee.temps }}\"><br>\r\n            <input class=\"sub\" type=\"submit\" value=\"Modifier\" style=\"width: 100px; height: 40px;\">\r\n\r\n            <a class=\"Annula\" href=\"/conteneur/show\">Annuler</a>\r\n        </fieldset>\r\n    </form>\r\n{% endblock %}\r\n
