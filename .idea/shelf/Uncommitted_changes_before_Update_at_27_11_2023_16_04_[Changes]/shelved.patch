<+>{% extends 'layout.html' %}\r\n\r\n{% block title %}\r\n    <title>modifier une collecte</title>\r\n{% endblock %}\r\n\r\n{% block body %}\r\n    <h1 class=\"titre\">MODIFIER UNE COLLECTE</h1>\r\n    <hr>\r\n    {% if error %}\r\n        <p><strong>Error:</strong> {{ error }}\r\n    {% endif %}\r\n\r\n    <div>\r\n        <div>\r\n            <form method=\"post\" action=\"/collecte/edit\" >\r\n                <fieldset class=\"formulaire\">\r\n\r\n                    <div>\r\n                        <label for=\"quantite\">Quantité collectée (kg)</label>\r\n                        <input type=\"number\" id=\"quantite\" name=\"quantite\" value=\"{{ collecte.quantite }}\" required/>\r\n                    </div>\r\n\r\n                    <div>\r\n                        <label for=\"type\">Type de déchet</label>\r\n                        <select name=\"type\" id=\"type\" required>\r\n                            {% for j in typeDechet %}\r\n                                <option value=\"{{ j.id }}\"\r\n                                        {% if typeDechet is defined and j.id ==  typeDechet.id %}selected{% endif %}\r\n                                > {{ j.libelle }} </option>\r\n                            {% endfor %}\r\n                        </select>\r\n                    </div>\r\n\r\n                    <div>\r\n                        <label for=\"centre\">Centre de collecte</label>\r\n                        <select name=\"centre\" id=\"centre\" required>\r\n                            {% for i in centreCollect %}\r\n                                <option value=\"{{ i.id }}\"\r\n                                        {% if centre is defined and i.id ==  centre %}selected{% endif %}\r\n                                > {{ i.lieu }} </option>\r\n                            {% endfor %}\r\n                        </select>\r\n                    </div>\r\n\r\n                    <div>\r\n                        <label for=\"tournee\">Tournee</label>\r\n                        <select name=\"tournee\" id=\"tournee\" required>\r\n                            {% for k in tournee %}\r\n                                <option value=\"{{ k.id }}\"\r\n                                        {% if tournee is defined and k.id ==  tournee %}selected{% endif %}\r\n                                > {{ k.date }} </option>\r\n                            {% endfor %}\r\n                        </select>\r\n                    </div>\r\n\r\n                    <div>\r\n                        <a href=\"/collecte/show\">Annuler</a>\r\n                        <button type=\"submit\" value=\"Éditer\" name=\"Éditer\">Éditer</button>\r\n                    </div>\r\n\r\n                </fieldset>\r\n            </form>\r\n        </div>\r\n    </div>\r\n{% endblock %}
