"tournee\" required>\r\n                            {% for k in tournee %}\r\n                                <option value=\"{{ k.id }}\"\r\n                                        {% if tournee is defined and k.id ==  tournee %}selected{% endif %}\r\n                                ></option>\r\n                            {% endfor %}\r\n                        </select>\r\n                    </div>\r\n\r\n                    <div>\r\n                        <a href=\"/collecte/show\">Annuler</a>\r\n                        <button type=\"submit\">Ajouter</button>\r\n                    </div>\r\n\r\n                </fieldset>\r\n            </form>\r\n        </div>\r\n    </div>\r\n{% endblock %}
