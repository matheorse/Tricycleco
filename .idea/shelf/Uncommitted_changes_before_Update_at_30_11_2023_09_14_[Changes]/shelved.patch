ollect %}\r\n                             {% if i.id == type.centre %}\r\n                               {{ i.lieu }} ({{i.id}})\r\n                             {% endif %}\r\n                           {% endfor %}</td>\r\n\r\n                       <td class=\"colonne\">{% for k in tournee %}\r\n                             {% if k.id == type.tournee %}\r\n                               {{ k.date }} ({{k.id}})\r\n                             {% endif %}\r\n                           {% endfor %}</td>\r\n\r\n                       <td class=\"colonne\">\r\n                          <a id=\"btn1\" class=\"éditer\" href=\"/collecte/edit?id={{ type.id }}&amp;param2={{type.quantite}}\">Editer</a>&nbsp;\r\n                          <a id=\"btn2\" class=\"supprimer\" href=\"/collecte/delete?id={{ type.id }}&amp;libelle={{ type.quantite }}\">Supprimer</a>\r\n                       </td>\r\n                   </tr>\r\n                {% endfor %}\r\n            {% else %}\r\n                    <tr>\r\n                          <td>\r\n                              Aucune collecte\r\n                          </td>\r\n                   </tr>\r\n            {% endif %}\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n    </body>\r\n{% endblock %}
